
public class Teacher {
	private String name;
	public String subject;
	private byte age;
	void teach() {}
	void takeAttendance() {}
	Teacher(String name, byte age,String subject){
		setName(name);
		setAge(age);
		setSubject(subject);
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getSubject() {
		return subject;
	}
	public void setSubject(String subject) {
		this.subject = subject;
	}
	public byte getAge() {
		return age;
	}
	public void setAge(byte age) {
		this.age = age;
	}
	
}
